<?xml version="1.0"?>
<!--from the bocian by AJ MacLeod-->
<PropertyList include="ask21-sound.xml">
	<fx>
		
		<wheelbrake>
			<name>wheelbrake</name>
			<mode>looped</mode>
			<path>Aircraft/ASK21/Sounds/wheelbrake.wav</path>
			<condition>
				<and>
					<property>/gear/gear[1]/wow</property>
					<greater-than>
						<property>/fcs/wheelbrake-cmd</property>
						<value>0.05</value>
					</greater-than>
				</and>
			</condition>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>0.05</factor>
			</volume>
		</wheelbrake>
		<!--Wind outside-->
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>velocities/airspeed-kt</property>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.005</factor>
				<min>0.0</min>
				<max>1</max>
				<offset>-0.2</offset>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1</offset>
			</pitch>
		</wind>
		<!--Wind in cockpit - front-->
		<wind>
			<name>wind front</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>velocities/airspeed-kt</property>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>fdm/jsbsim/aero/qbar-psf</property>
				<factor>0.2</factor>
				<min>0.0</min>
				<max>5</max>
				<offset>-0.2</offset>
			</volume>
			<volume>
				<property>sim/model/door-positions/frontwin/position-norm</property>
				<interpolation>
					<entry>
						<ind>0.0</ind>
						<dep>0.25</dep>
					</entry>
					<entry>
						<ind>0.1</ind>
						<dep>0.65</dep>
					</entry>
					<entry>
						<ind>1</ind>
						<dep>0.75</dep>
					</entry>
				</interpolation><!--
				<factor>0.95</factor>
				<min>0.05</min>
				<max>1</max>
				<offset>0.05</offset>-->
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1.1</offset>
			</pitch>
			<position>
				<x>-2.8</x>
				<y>0.0</y>
				<z>0.8</z>
			</position>
			<reference-dist>0.5</reference-dist>
			<max-dist>2</max-dist>
		</wind>
		<!--Wind in cockpit - rear-->
		<wind>
			<name>wind rear</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>velocities/airspeed-kt</property>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<min>0.03</min>
				<max>5</max>
				<offset>-0.2</offset>
			</volume>
			<volume>
				<property>sim/model/door-positions/rearwin/position-norm</property>
				<interpolation>
					<entry>
						<ind>0.0</ind>
						<dep>0.25</dep>
					</entry>
					<entry>
						<ind>0.1</ind>
						<dep>0.65</dep>
					</entry>
					<entry>
						<ind>1</ind>
						<dep>0.75</dep>
					</entry>
				</interpolation><!--
				<factor>0.95</factor>
				<min>0.05</min>
				<max>1</max>
				<offset>0.05</offset>-->
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1.05</offset>
			</pitch>
			<position>
				<x>-2</x>
				<y>0.0</y>
				<z>0.9</z>
			</position>
			<reference-dist>0.5</reference-dist>
			<max-dist>2</max-dist>
		</wind>
		
	</fx>
</PropertyList>
