<?xml version="1.0"?>
<PropertyList>
	
	<filter>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<condition>
				<equals>
					<property>/sim/flight-model</property>
					<value>jsb</value>
				</equals>
			</condition>					
			<property>/engines/engine/rpm</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/sim/flight-model</property>
					<value>yasim</value>
				</equals>
			</condition>
			<expression>
				<product>
					<property>/engines/engine/rpm</property>
					<value>2.68</value>
				</product>
			</expression>
		</input>
		<gain>1</gain>
		<output>/engines/engine/engine-rpm</output>
	</filter>
	
	<filter>
		<type>gain</type>
		<input>
			<condition>
				<equals>
					<property>/engines/engine/prop-pos-norm</property>
					<value>1.0</value>
				</equals>
			</condition>
			<property>/engines/engine/engine-rpm</property>
		</input>
		<input>0.0</input>
		<gain>1.0</gain>
		<output>/engines/engine/engine-rpm-checked</output>
	</filter>
	
</PropertyList>
